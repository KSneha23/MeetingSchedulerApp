<%- include("partials/header"); -%>

<div class="container blogs mt-5">
  <div class="container box text-center heading">
    <h3>Your Schedule</h3>
  </div>
  <div class="box">
    <% if(newMeetings.length>0){ %>
      <% for(let i=0; i<newMeetings.length; i++){ %>
            <p class="mb-0 fw-bold"><%=newMeetings[i].title%></p>
            <p class="mt-0 mb-0 fw-bold text-secondary"><%=newMeetings[i].details%></p>
            <p class="mt-0 fw-bold text-secondary"><%=newMeetings[i].id%></p>
      <% } %>
    <% } else{ %>
      <h5 class="text-center text-muted">Yo!! No schedule Today</h5>
    <% } %>
  </div>
</div>

<%- include("partials/footer"); -%>
